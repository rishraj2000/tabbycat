#, python-format
msgid "Venue does not meet any constraint of %(name)s"
msgstr "Аудитория не подходит ни под одно из ограничений из %(name)s"

#: venues/utils.py:44
#, python-format
msgid "Venue constraint of %(team)s met (%(category)s, via institution %(institution)s)"
msgstr "Ограничение аудиторий для %(team)s соответсвуте (%(category)s через институт %(institution)s)"

#: venues/utils.py:45
#, python-format
msgid "Venue does not meet any constraint of institution %(institution)s (%(team)s)"
msgstr "Место не подходит ни под одно ограничение института %(institution)s (%(team)s)"

